- console.log( rows )

- keys = Object.keys( rows[ 0 ] )

table.table.table-sm.table-bordered.table-dark
    thead
        tr
            - for (var i  = 0; i < keys.length; i++ )
                th= keys[ i ]
    tbody
        - for(var i = 0; i < rows.length; i++ )
            tr.bg-primary
                - for (var j = 0; j < keys.length; j++ )
                    td= rows[ i ][ keys[ j ] ]